{
  "name": "@quantum-forge/tik-tok-business-sdk",
  "version": "0.0.2",
  "description": "Modern TypeScript SDK for the TikTok Business API with strict type checking and ESM support",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "test": "mocha --require ts-node/register --recursive \"test/**/*.ts\"",
    "prepare": "npm run build",
    "convert-to-ts": "for %f in (src/**/*.js) do move \"%f\" \"%~dpnf.ts\"",
    "convert": "node convert-to-ts.js",
    "clean": "rimraf dist",
    "bundle": "rspack"
  },
  "browser": {
    "fs": false
  },
  "dependencies": {
    "superagent": "^9.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.26.9",
    "@babel/plugin-proposal-decorators": "^7.0.0",
    "@babel/plugin-proposal-do-expressions": "^7.0.0",
    "@babel/plugin-proposal-export-default-from": "^7.0.0",
    "@babel/plugin-proposal-function-bind": "^7.0.0",
    "@babel/plugin-proposal-function-sent": "^7.0.0",
    "@babel/plugin-proposal-pipeline-operator": "^7.0.0",
    "@babel/plugin-proposal-throw-expressions": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-syntax-import-meta": "^7.0.0",
    "@babel/plugin-transform-class-properties": "^7.0.0",
    "@babel/plugin-transform-export-namespace-from": "^7.0.0",
    "@babel/plugin-transform-json-strings": "^7.0.0",
    "@babel/plugin-transform-logical-assignment-operators": "^7.0.0",
    "@babel/plugin-transform-nullish-coalescing-operator": "^7.0.0",
    "@babel/plugin-transform-numeric-separator": "^7.0.0",
    "@babel/plugin-transform-optional-chaining": "^7.0.0",
    "@babel/preset-env": "^7.26.9",
    "@babel/preset-typescript": "^7.26.0",
    "@babel/register": "^7.0.0",
    "@rspack/cli": "^1.2.5",
    "@rspack/core": "^1.2.5",
    "@types/estree": "^1.0.6",
    "@types/expect.js": "^0.3.29",
    "@types/http-errors": "^2.0.4",
    "@types/json-schema": "^7.0.15",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.0.0",
    "@types/range-parser": "^1.2.7",
    "@types/retry": "^0.12.5",
    "@types/send": "^0.17.4",
    "@types/serve-index": "^1.9.4",
    "@types/serve-static": "^1.15.7",
    "@types/sinon": "^10.0.0",
    "@types/sockjs": "^0.3.36",
    "@types/superagent": "^4.1.15",
    "@types/ws": "^8.5.14",
    "babel-loader": "^9.2.1",
    "expect.js": "^0.3.1",
    "glob": "^9.0.0",
    "lru-cache": "^7.0.0",
    "mocha": "^8.0.1",
    "rimraf": "^5.0.0",
    "sinon": "^7.2.0",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.0.0",
    "typescript": "^4.5.0",
    "webpack-bundle-analyzer": "^4.10.2"
  },
  "main": "index.js",
  "repository": "https://github.com/Evolving-Software/quantum-forge-tik-tok-business-sdk.git",
  "author": "Riley Seaburg <riley@rileyseaburg.com>",
  "license": "MIT",
  "private": false
}
